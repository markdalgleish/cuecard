var cuecard = require('cuecard'),
	path = require('path');

cuecard.create({
	adapter: '<%=adapter%>',
	port: <%=port%>,
	remoteUrl: <%= hasRemote ? "'" + remoteUrl + "'" : false %>,
	title: '<%=title%>',
	static: 'static',
	slides: path.join(__dirname, 'slides.html'),
	styles: path.join(__dirname, 'slides.css')
});